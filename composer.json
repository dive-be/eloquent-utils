{
    "name": "dive-be/eloquent-utils",
    "description": "Declarative Eloquent utilities for an improved DX",
    "keywords": [
        "dive",
        "eloquent",
        "support",
        "utilities",
        "declarative"
    ],
    "homepage": "https://github.com/dive-be/eloquent-utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammed Sari",
            "email": "muhammed@dive.be",
            "homepage": "https://dive.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.2",
        "illuminate/database": "^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16"
    },
    "autoload": {
        "psr-4": {
            "Dive\\Eloquent\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
